{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@layout/*": [
                "src/layout/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@client/*": [
                "client/*"
            ],
            "@/*": [
                "src/*"
            ]
        }
    }
}